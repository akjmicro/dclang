: test_isalnum
  "k" ord isalnum     assert
  "J" ord isalnum     assert
  "1" ord isalnum     assert
  "9" ord isalnum     assert
  ";" ord isalnum not assert
;

: test_isalpha
  "k" ord isalpha     assert
  "9" ord isalpha not assert
  ";" ord isalpha not assert
;

: test_iscntrl
  "k" ord iscntrl not assert
  20 iscntrl     assert
;

: test_isdigit
  "k" ord isdigit not assert
  "J" ord isdigit not assert
  "!" ord isdigit not assert
  "1" ord isdigit     assert
  "7" ord isdigit     assert
;

: test_isgraph
  "\n" ord isgraph not assert
  " "  ord isgraph not assert
  "!"  ord isgraph     assert
  "1"  ord isgraph     assert
  "7"  ord isgraph     assert
;

: test_islower
  "k" ord islower     assert
  "K" ord islower not assert
  "a" ord islower     assert
  "A" ord islower not assert
  "7" ord islower not assert
;

: test_isprint
  "k"  ord isprint     assert
  "\n" ord isprint not assert
  3        isprint not assert
;

: test_ispunct
  "!" ord ispunct     assert
  ";" ord ispunct     assert
  "0" ord ispunct not assert
  "X" ord ispunct not assert
;

: test_isspace
  " "  ord isspace     assert
  "\n" ord isspace     assert
  "\t" ord isspace     assert
  " "  ord isspace     assert
  "5"  ord isspace not assert
  "F"  ord isspace not assert
;

: test_isupper
  "k" ord isupper not assert
  "K" ord isupper     assert
  "a" ord isupper not assert
  "A" ord isupper     assert
  "7" ord isupper not assert
;

: test_isxdigit
  "0" ord isxdigit     assert
  "5" ord isxdigit     assert
  "9" ord isxdigit     assert
  "A" ord isxdigit     assert
  "B" ord isxdigit     assert
  "C" ord isxdigit     assert
  "D" ord isxdigit     assert
  "E" ord isxdigit     assert
  "F" ord isxdigit     assert
  "a" ord isxdigit     assert
  "b" ord isxdigit     assert
  "c" ord isxdigit     assert
  "d" ord isxdigit     assert
  "e" ord isxdigit     assert
  "f" ord isxdigit     assert
  # exceptions
  "G"  ord isxdigit not assert
  "g"  ord isxdigit not assert
  "!"  ord isxdigit not assert
  ":"  ord isxdigit not assert
  "\n" ord isxdigit not assert
;

test_isalnum
test_isalpha
test_iscntrl
test_isdigit
test_isgraph
test_isgraph
test_isgraph
test_isgraph
test_islower
test_isprint
test_ispunct
test_isspace
test_isupper
test_isxdigit

"If you are only seeing this msg, all tests have passed!" print cr
