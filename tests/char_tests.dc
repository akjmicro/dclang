: test_isalnum
  "Testing isalnum" print cr
  "k" ord isalnum 1 = assert
  "J" ord isalnum 1 = assert
  "1" ord isalnum 1 = assert
  "9" ord isalnum 1 = assert
  ";" ord isalnum 0 = assert
;

: test_isalpha
  "Testing isalpha" print cr
  "k" ord isalpha 1 = assert
  "9" ord isalpha 0 = assert
  ";" ord isalpha 0 = assert
;

: test_iscntrl
  "Testing iscntrl" print cr
  "k" ord iscntrl 0 = assert
  20 iscntrl 1 = assert
;

: test_isdigit
  "Testing isdigit" print cr
  "k" ord isdigit 0 = assert
  "J" ord isdigit 0 = assert
  "!" ord isdigit 0 = assert
  "1" ord isdigit 1 = assert
  "7" ord isdigit 1 = assert
;

: test_isgraph
  "Testing isgraph" print cr
  "\n" ord isgraph 0 = assert
  " " ord isgraph 0 = assert
  "!" ord isgraph 1 = assert
  "1" ord isgraph 1 = assert
  "7" ord isgraph 1 = assert
;

: test_islower
  "Testing islower" print cr
  "k" ord islower 1 = assert
  "K" ord islower 0 = assert
  "a" ord islower 1 = assert
  "A" ord islower 0 = assert
  "7" ord islower 0 = assert
;

: test_isprint
  "Testing isprint" print cr
  "k" ord isprint 1 = assert
  "\n" ord isprint 0 = assert
  3 isprint 0 = assert
;

: test_ispunct
  "Testing ispunct" print cr
  "!" ord ispunct 1 = assert
  ";" ord ispunct 1 = assert
  "0" ord ispunct 0 = assert
  "X" ord ispunct 0 = assert
;

: test_isspace
  "Testing isspace" print cr
  " " ord isspace 1 = assert
  "\n" ord isspace 1 = assert
  "\t" ord isspace 1 = assert
  " " ord isspace 1 = assert
  "5" ord isspace 0 = assert
  "F" ord isspace 0 = assert
;

: test_isupper
  "Testing isupper" print cr
  "k" ord isupper 0 = assert
  "K" ord isupper 1 = assert
  "a" ord isupper 0 = assert
  "A" ord isupper 1 = assert
  "7" ord isupper 0 = assert
;

: test_isxdigit
  "Testing isxdigit" print cr
  "0" ord isxdigit 1 = assert
  "5" ord isxdigit 1 = assert
  "9" ord isxdigit 1 = assert
  "A" ord isxdigit 1 = assert
  "B" ord isxdigit 1 = assert
  "C" ord isxdigit 1 = assert
  "D" ord isxdigit 1 = assert
  "E" ord isxdigit 1 = assert
  "F" ord isxdigit 1 = assert
  "a" ord isxdigit 1 = assert
  "b" ord isxdigit 1 = assert
  "c" ord isxdigit 1 = assert
  "d" ord isxdigit 1 = assert
  "e" ord isxdigit 1 = assert
  "f" ord isxdigit 1 = assert
  # exceptions
  "G" ord isxdigit 0 = assert
  "g" ord isxdigit 0 = assert
  "!" ord isxdigit 0 = assert
  ":" ord isxdigit 0 = assert
  "\n" ord isxdigit 0 = assert
;

test_isalnum
test_isalpha
test_iscntrl
test_isdigit
test_isgraph
test_isgraph
test_isgraph
test_isgraph
test_islower
test_isprint
test_ispunct
test_isspace
test_isupper
test_isxdigit
