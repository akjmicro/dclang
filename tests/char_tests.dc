: test_isalnum
  "k" isalnum     assert
  "J" isalnum     assert
  "1" isalnum     assert
  "9" isalnum     assert
  ";" isalnum not assert
;

: test_isalpha
  "k" isalpha     assert
  "9" isalpha not assert
  ";" isalpha not assert
;

: test_iscntrl
  "k"       iscntrl not assert
  "\u0014"  iscntrl     assert
;

: test_isdigit
  "k" isdigit not assert
  "J" isdigit not assert
  "!" isdigit not assert
  "1" isdigit     assert
  "7" isdigit     assert
;

: test_isgraph
  "\n" isgraph not assert
  " "  isgraph not assert
  "!"  isgraph     assert
  "1"  isgraph     assert
  "7"  isgraph     assert
;

: test_islower
  "k" islower     assert
  "K" islower not assert
  "a" islower     assert
  "A" islower not assert
  "7" islower not assert
;

: test_isprint
  "k"      isprint     assert
  "\n"     isprint not assert
  "\u0003" isprint not assert
;

: test_ispunct
  "!" ispunct     assert
  ";" ispunct     assert
  "0" ispunct not assert
  "X" ispunct not assert
;

: test_isspace
  " "  isspace     assert
  "\n" isspace     assert
  "\t" isspace     assert
  " "  isspace     assert
  "5"  isspace not assert
  "F"  isspace not assert
;

: test_isupper
  "k" isupper not assert
  "K" isupper     assert
  "a" isupper not assert
  "A" isupper     assert
  "7" isupper not assert
;

: test_isxdigit
  "0" isxdigit     assert
  "5" isxdigit     assert
  "9" isxdigit     assert
  "A" isxdigit     assert
  "B" isxdigit     assert
  "C" isxdigit     assert
  "D" isxdigit     assert
  "E" isxdigit     assert
  "F" isxdigit     assert
  "a" isxdigit     assert
  "b" isxdigit     assert
  "c" isxdigit     assert
  "d" isxdigit     assert
  "e" isxdigit     assert
  "f" isxdigit     assert
  # exceptions
  "G"  isxdigit not assert
  "g"  isxdigit not assert
  "!"  isxdigit not assert
  ":"  isxdigit not assert
  "\n" isxdigit not assert
;

test_isalnum
test_isalpha
test_iscntrl
test_isdigit
test_isgraph
test_isgraph
test_isgraph
test_isgraph
test_islower
test_isprint
test_ispunct
test_isspace
test_isupper
test_isxdigit

"If you are only seeing this msg, all tests have passed!" print cr
