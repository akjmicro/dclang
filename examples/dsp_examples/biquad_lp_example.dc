"dsp.dc" import

make_biquad myflt

: biquad_lp_example
  inf times
    # some noise, scaled to 0.1 max amplitude
      noise 0.1 *
        myflt
        false
          0.2 sine
          500 *
          1500 +
            29.99
      biquad_lp
    # scale down, it's louder
      0.07 *
    # mix into center
        0.5
    panmix
    # put to output
    stereo_out
    # advance counter
    t+
  again
;

biquad_lp_example
