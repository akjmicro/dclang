: fibo-setup
  # this allows us to set up a loop withouth the user worrying about it
  1 0     # ( n 0 1 )
  rot     # ( 1 0 n )
  0 1     # ( 1 0 n 0 1 )
;

: fibonacci
  dup 0 =
  if
    0 . cr drop
    return
  else
    fibo-setup
    for
      tuck +
    next
    . cr drop
  endif
;

: fibonacci-benchmark
  clock .s
  24 fibonacci .s
  clock .s
  swap .s - .s
  "24 fibonacci finished in: " . "microseconds" print cr
;


fibonacci-benchmark
