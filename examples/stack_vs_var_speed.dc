: time_stack
  "Timing the stack:                  " print
  clock
  1
  25000000 times
    dup drop
  again
  drop
  clock swap - 2 6 .rj cr
;

time_stack

: time_svstack
  "Timing the save stack:             " print
  clock
  1 svpush
  25000000 times
    0 svpick drop
  again
  clock swap - 2 6 .rj cr
;

time_svstack

1 4 !

: time_var_raw_int
  "Timing variables using integers:   " print
  clock
  25000000 times
    4 @ drop
  again
  clock swap - 2 6 .rj cr
;

time_var_raw_int

var myvar
1 myvar !

: time_var
  "Timing normal named variables:     " print
  clock
  25000000 times
    myvar @ drop
  again
  clock swap - 2 6 .rj cr
;

time_var

