: time_tree
  "Timing tree lookup:                " print
  tmake 0 !
  clock
  0 "mykey" 23 t! drop
  25000000 times
    0 "mykey" t@ drop
  again
  clock swap - 2 6 .rj cr
;

time_tree

: do_math_swap
  "Timing math with swap:             " print
  clock
    25000000 times
        1 2 +
          3 4 +
          swap
        /
      drop
    again
  clock swap - 2 6 .rj cr
;

do_math_swap

: do_math_save
  "Timing math with svpush and svpop: " print
  clock
    25000000 times
        1 2 + svpush
        3 4 +
          svpop
        /
      drop
    again
  clock swap - 2 6 .rj cr
;

do_math_save

: do_math_var
  "Timing math with ! and @:          " print
  clock
    25000000 times
        1 2 + 0 !
        3 4 +
          0 @
        /
      drop
    again
  clock swap - 2 6 .rj cr
;

do_math_var
